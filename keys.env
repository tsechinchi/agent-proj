# API keys and credentials (replace placeholders with real values)
OPENWEATHER_API_KEY="your_key_here"
GOOGLE_CLOUD_API_KEY="your_key_here"

# Google Custom Search (one of these should be set)
GOOGLE_CSE_ID="your_cse_id_here"
GOOGLE_CUSTOM_SEARCH_CX="your_cx_here"
GOOGLE_CX="your_cx_alias_here"

# Google API fallback key
GOOGLE_API_KEY="your_key_here"

# Amadeus (flight & hotel APIs)
AMADEUS_CLIENT_ID="your_amadeus_client_id"
AMADEUS_CLIENT_SECRET="your_amadeus_client_secret"
AMADEUS_ENV="test"

# OpenAI / LLM / OpenRouter
OPENAI_API_KEY="sk-your-api-key"

# Google application credentials (path to service account JSON)
GOOGLE_APPLICATION_CREDENTIALS="/path/to/service_account.json"
